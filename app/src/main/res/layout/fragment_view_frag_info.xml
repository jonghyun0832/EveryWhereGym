<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:context=".ViewFragInfo">


    <TextView
        android:id="@+id/textview_showPF_intro_fg"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="20dp"
        android:background="@drawable/textbox_edge"
        android:gravity="left|center_vertical"
        android:paddingLeft="20dp"
        android:paddingTop="10dp"
        android:paddingRight="20dp"
        android:paddingBottom="10dp"
        android:text="소개글"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/hardtext_tr_mypage_expert_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:text="전문분야"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textview_showPF_expert_fg"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/textview_showPF_expert_fg" />

    <TextView
        android:id="@+id/hardtext_tr_mypage_editcareer_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="경력사항"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textview_showPF_career_fg"
        app:layout_constraintStart_toStartOf="@+id/hardtext_tr_mypage_expert_edit"
        app:layout_constraintTop_toTopOf="@+id/textview_showPF_career_fg" />

    <TextView
        android:id="@+id/hardtext_tr_mypage_certify_edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="자격사항"
        android:textColor="@color/black"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="@+id/textview_showPF_certify_fg"
        app:layout_constraintStart_toStartOf="@+id/hardtext_tr_mypage_editcareer_edit"
        app:layout_constraintTop_toTopOf="@+id/textview_showPF_certify_fg" />

    <TextView
        android:id="@+id/textview_showPF_expert_fg"
        android:layout_width="180dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:background="@drawable/text_edge_all"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="@+id/guideline38"
        app:layout_constraintTop_toBottomOf="@+id/textview_showPF_intro_fg" />

    <TextView
        android:id="@+id/textview_showPF_career_fg"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/text_edge_all"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/textview_showPF_expert_fg"
        app:layout_constraintStart_toStartOf="@+id/textview_showPF_expert_fg"
        app:layout_constraintTop_toBottomOf="@+id/textview_showPF_expert_fg" />

    <TextView
        android:id="@+id/textview_showPF_certify_fg"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:background="@drawable/text_edge_all"
        android:paddingLeft="10dp"
        android:paddingTop="10dp"
        android:paddingBottom="10dp"
        android:text="TextView"
        android:textColor="@color/black"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="@+id/textview_showPF_career_fg"
        app:layout_constraintStart_toStartOf="@+id/textview_showPF_career_fg"
        app:layout_constraintTop_toBottomOf="@+id/textview_showPF_career_fg" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline38"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="160dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textview_showPF_certify_fg" />


</androidx.constraintlayout.widget.ConstraintLayout>